# LDAP alias support

server_host = ldap://127.0.0.1:389
search_base = dc=example,dc=com
query_filter = (&(!(kolabDeleteFlag=*))(|(alias=%s)(mail=%s)))
result_attribute = mail
result_filter = %s
search_timeout = 15
scope = sub
bind = yes
bind_dn = cn=nobody,cn=internal,dc=example,dc=com
bind_pw = PASSWORD
version = 3
